{
  "name": "endosso-job-fgts",
  "version": "1.0.0",
  "description": "Sistema de processamento de documentos FGTS",
  "main": "src/index.js",
  "scripts": {
    "start": "node --max-old-space-size=8192 --expose-gc src/index.js",
    "dev": "nodemon --exec \"node --max-old-space-size=8192 --expose-gc\" src/index.js",
    "test-memory": "node --max-old-space-size=8192 --expose-gc scripts/test-memory.js",
    "start:monitor": "node --max-old-space-size=8192 --expose-gc --trace-gc src/index.js"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "docx-templates": "^4.9.2",
    "pdf-lib": "^1.17.1",
    "fs-extra": "^11.1.1",
    "libreoffice-convert": "^1.4.1",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "fgts",
    "documentos",
    "processamento"
  ],
  "author": "Theodoro Vieira",
  "license": "MIT"
}
